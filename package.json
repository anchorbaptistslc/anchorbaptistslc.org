{
  "name": "anchorbaptistslc-website",
  "version": "1.0.0",
  "description": "Website for Anchor Baptist Church in Salt Lake City",
  "main": "index.js",
  "scripts": {
    "build": "hugo --theme=anchor-theme --minify --gc",
    "start": "hugo serve --theme=anchor-theme --buildDrafts --buildFuture",
    "dev": "hugo serve --theme=anchor-theme --buildDrafts --buildFuture --disableFastRender --printPathWarnings",
    "clean": "rm -rf public resources",
    "build:prod": "npm run clean && NODE_ENV=production HUGO_ENV=production hugo --theme=anchor-theme --gc --minify"
  },
  "keywords": [
    "church",
    "website",
    "hugo",
    "static-site"
  ],
  "author": "Anchor Baptist Church",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "prettier": "^3.0.3"
  }
}
